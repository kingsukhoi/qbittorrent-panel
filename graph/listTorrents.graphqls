scalar Int64

type File {
    Availability: Float!
    Index: Int!
    IsSeed: Boolean!
    Name: String!
    PieceRange: [Int!]!
    Priority: Int!
    Progress: Float!
    SizeBytes: Int64!
}

type Torrent {
    Client: String!
    Name: String!
    Category: String!
    Ratio: Float!
    InfoHashV1: String!
    Comment: String!
    RootPath: String!
    SavePath: String!
    SizeBytes: Int64!
    Tracker: String!
    Files: [File!]!
}

type Query {
    Torrents: [Torrent!]!
}